<Window x:Name="ProgressBarWindow" x:Class="AKCWPF.LoadingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AKCWPF"
        mc:Ignorable="d" WindowStartupLocation="CenterOwner" ResizeMode="NoResize" Topmost="True" d:DesignWidth="1618" d:DesignHeight="261" Width="299" Height="100" ShowInTaskbar="False">

    <Grid Margin="20">
        <ProgressBar Minimum="0" Maximum="100" Name="pbStatus" IsIndeterminate="True" Background="#FFE6E6E6" BorderThickness="1" />
    </Grid>
</Window>